/dts-v1/;

/ {
	le-dt-id = "gxl_p212_1g";
	model = "Amlogic";
	compatible = "amlogic, Gxbb";
	amlogic-dt-id = "gxl_p212_1g";
	interrupt-parent = < 0x01 >;
	#address-cells = < 0x02 >;
	#size-cells = < 0x02 >;

	gpu: mali@d00c0000 {
		#cooling-cells = < 0x02 >;	/* min followed by max */
		compatible = "arm,mali-450";
		interrupt-parent = < &gic >;
		reg = < 0 0xd00c0000 0 0x40000 >, /*mali APB bus base address*/
			 < 0 0xc1104440 0 0x01000 >, /*reset register*/
			 < 0 0xc8100000 0 0x01000 >, /*aobus base address for gpu pmu domain*/
			 < 0 0xc883c000 0 0x01000 >, /*hiubus base address for gpu clk cntl*/
			 < 0 0xc1104440 0 0x01000 >;
		interrupts = < 0 160 4 >, < 0 161 4 >, < 0 162 4 >, < 0 163 4 >,
				< 0 164 4 >, < 0 165 4 >, < 0 166 4 >, < 0 167 4 >,
				< 0 168 4 >, < 0 169 4 >;
		interrupt-names = "IRQGP", "IRQGPMMU", "IRQPP", "IRQPMU",
			"IRQPP0", "IRQPPMMU0", "IRQPP1", "IRQPPMMU1",
			"IRQPP2", "IRQPPMMU2";
		pmu_domain_config = < 0x01 0x02 0x04 0x04 0x00 0x00 0x00 0x00 0x00 0x01 0x02 0x00 >;
		pmu_switch_delay = < 0xffff >;
		num_of_pp = < 0x03 >;
		def_clock = < 0x03 >;
		sc_mpp = < 0x03 >;	/* number of pp used most of time.*/
		tbl = < &clk125_cfg &clk285_cfg &clk400_cfg &clk500_cfg &clk666_cfg &clk750_cfg >;

		clocks = < &clock CLK_FPLL_DIV3 >,
			< &clock CLK_FPLL_DIV4 >,
			< &clock CLK_FPLL_DIV5 >,
			< &clock CLK_FPLL_DIV7 >,
			< &clock GP0_PLL >,
			< &clock CLK_MALI >,
			< &clock CLK_MALI_0 >,
			< &clock CLK_MALI_1 >;
		clock-names = "fclk_div3", "fclk_div4", "fclk_div5", "fclk_div7", "gp0_pll", "clk_mali", "clk_mali_0", "clk_mali_1";

		/*control_interval x keep_count =  = 900 - 1000ms */
		control_interval = < 0xc8 >; //200

		 clk125_cfg: clk125_cfg {
			clk_freq = < 0x7735940 >; //125000000
			clk_parent = "fclk_div4";
			clkp_freq = < 0x1dcd6500 >; //500000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x05 >;
			threshold = < 0x1e 0xfa >; //30 250
		};

		 clk250_cfg: clk250_cfg {
			clk_freq = < 0xee6b280 >; //250000000
			clk_parent = "fclk_div4";
			clkp_freq = < 0x1dcd6500 >; //500000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x05 >;
			threshold = < 0x73 0xfa >; //115 250
			/*125 = 250*(125/250), 50 = 60-10*/
		};

		 clk285_cfg: clk285_cfg {
			clk_freq = < 0x10fcc140 >; //285000000
			clk_parent = "fclk_div7";
			clkp_freq = < 0x10fcc140 >; //285000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x05 >;
			threshold = < 0x64 0xfa >; //100 250
			/*109 = 250*(125/285)*/
		};

		 clk400_cfg: clk400_cfg {
			clk_freq = < 0x17d78400 >; //400000000
			clk_parent = "fclk_div5";
			clkp_freq = < 0x17d78400 >; //400000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x03 >;
			threshold = < 0xa8 0xfa >; //168 250
			/*178 = 250*(285/400)*/
		};

		 clk500_cfg: clk500_cfg {
			clk_freq = < 0x1dcd6500 >; //500000000
			clk_parent = "fclk_div4";
			clkp_freq = < 0x1dcd6500 >; //500000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x02 >;
			threshold = < 0xbe 0xfa >; //190 250
			/*200 = 250*(400/500)*/
		};

		 clk666_cfg: clk666_cfg {
			clk_freq = < 0x27b25a80 >; //666000000
			clk_parent = "fclk_div3";
			clkp_freq = < 0x27b25a80 >; //666000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x01 >;
			threshold = < 0xb1 0xfa >; //177 250
			/*187.5 = 250*(500/666.6)*/
		};

		 clk750_cfg: clk750_cfg {
			clk_freq = < 0x2c588a00 >; //744000000
			clk_parent = "gp0_pll";
			clkp_freq = < 0x2c588a00 >; //744000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x01 >;
			threshold = < 0xd5 0xff >; //213 255
			/*223 = 250*(666.0/744.0), 223+7 = 230*/
		};

		 clk800_cfg: clk800_cfg {
			clk_freq = < 0x2f34f600 >; //792000000
			clk_parent = "gp0_pll";
			clkp_freq = < 0x2f34f600 >; //792000000
			voltage = < 0x47e >; //1150
			keep_count = < 0x01 >;
			threshold = < 0xe6 0xff >; //230 255
		};
	};

	cpus: cpus {
		#address-cells = < 0x02 >;
		#size-cells = < 0x00 >;
		#cooling-cells = < 2 >;	/* min followed by max */

		cpu0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = < 0x00 0x00 >;
			enable-method = "psci";
		};

		cpu1: cpu@1 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = < 0x00 0x01 >;
			enable-method = "psci";
		};

		cpu2: cpu@2 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = < 0x00 0x02 >;
			enable-method = "psci";
		};

		cpu3: cpu@3 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = < 0x00 0x03 >;
			enable-method = "psci";
		};
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts = < GIC_PPI 13 0xff01 >, 
				 < GIC_PPI 14 0xff01 >,
				 < GIC_PPI 11 0xff01 >,
				 < GIC_PPI 10 0xff01 >;
	};	/* GIC_PPI: 1*/

	timer_bc {
		compatible = "arm, meson-bc-timer";
		reg = < 0x00 0xc1109990 0x00 0x04
			0x00 0xc1109994 0x00 0x04 >;
		timer_name = "Meson TimerF";
		clockevent-rating = < 0x12c >;
		clockevent-shift = < 0x14 >;
		clockevent-features = < 0x23 >;
		interrupts = < 0x00 0x3c 0x01 >;
		bit_enable = < 0x10 >;
		bit_mode = < 0x0c >;
		bit_resolution = < 0x00 >;
	};

	arm_pmu {
		compatible = "arm,armv8-pmuv3";
		interrupts = < 0 137 4 >,
				 < 0 138 4 >,
				 < 0 153 4 >,
				 < 0 154 4 >;
	};

	meson_suspend: pm {
		compatible = "amlogic, pm";
		device_name = "aml_pm";
		gxbaby-suspend;
		reg = < 0x00 0xc81000a8 0x00 0x04
			0x00 0xc810023c 0x00 0x04 >;
	};

	gic: interrupt-controller@2c001000 {
		compatible = "arm,cortex-a15-gic", "arm,cortex-a9-gic";
		#interrupt-cells = < 0x03 >;
		#address-cells = < 0x00 >;
		interrupt-controller;
		reg = < 0x00 0xc4301000 0x00 0x1000
			0x00 0xc4302000 0x00 0x100 >;
		interrupts = < GIC_PPI 0x09 0xf04 >;	/* GIC_PPI: 1*/
	};

	aml_restart {
		compatible = "aml, restart";
		sys_reset = < 0x84000009 >;
		sys_poweroff = < 0x84000008 >;
	};

	psci {
		compatible = "arm,psci";
		method = "smc";
		cpu_suspend = < 0xc4000001 >;
		cpu_off = < 0x84000002 >;
		cpu_on = < 0xc4000003 >;
		migrate = < 0xc4000005 >;
	};

	secmon {
		compatible = "amlogic, secmon";
		memory-region = < &secmon_reserved >;
		in_base_func = < 0x82000020 >;
		out_base_func = < 0x82000021 >;
	};

	securitykey {
		compatible = "aml, securitykey";
		storage_query = < 0x82000060 >;
		storage_read = < 0x82000061 >;
		storage_write = < 0x82000062 >;
		storage_tell = < 0x82000063 >;
		storage_verify = < 0x82000064 >;
		storage_status = < 0x82000065 >;
		storage_list = < 0x82000067 >;
		storage_remove = < 0x82000068 >;
		storage_in_func = < 0x82000023 >;
		storage_out_func = < 0x82000024 >;
		storage_block_func = < 0x82000025 >;
		storage_size_func = < 0x82000027 >;
	};

	cpu_iomap {
		compatible = "amlogic, iomap";
		#address-cells = < 0x02 >;
		#size-cells = < 0x02 >;
		ranges;

		io_cbus_base {
			reg = < 0x00 0xc1100000 0x00 0x100000 >;
		};

		io_apb_base {
			reg = < 0x00 0xd0000000 0x00 0x200000 >;
		};

		io_aobus_base {
			reg = < 0x00 0xc8100000 0x00 0x100000 >;
		};
	};

	cpufreq-meson {
		compatible = "amlogic, cpufreq-scpi";
		status = "okay";
		clocks = < &scpi_dvfs 0 >;
		clock-names = "cpu_clk";
	};

	amlogic-watchdog {
		compatible = "amlogic, gx-wdt";
		status = "okay";
		default_timeout = < 0x0a >;
		reset_watchdog_method = < 1 >; //0: sysfs,1: kernel
		reset_watchdog_time = < 0x02 >;
		shutdown_timeout = < 0x0a >;
		firmware_timeout = < 0x06 >;
		suspend_timeout = < 0x06 >;
		reg = < 0x00 0xc11098d0 0x00 0x10 >;
		clocks = < &clock CLK_XTAL >;
	};

	amlogic-jtag {
		compatible = "amlogic, jtag";
		status = "disabled";
		pinctrl-names = "jtag_apao_pins", "jtag_apee_pins";
		pinctrl-0 = < &jtag_apao_pins >;
		pinctrl-1 = < &jtag_apee_pins >;
	};

	clock: meson_clock {
		compatible = "amlogic, gxl-clock";
		reg = < 0x0 0xc883c000 0x0 0x1000 >,	/* HIU BUS */
			 < 0x0 0xc8100000 0x0 0x1000 >;	/* AOBUS */
		#clock-cells = < 0x01 >;
		#reset-cells = < 0x01 >;
		sys_max = < 0x5b8d8000 >;
	};

	cpu_info {
		compatible = "amlogic, cpuinfo";
		cpuinfo_cmd = < 0x82000044 >;
	};

	pinmux: pinmux {
		compatible = "amlogic, pinmux-gxl";
		dev_name = "pinmux";
		#pinmux-cells = < 0x02 >;
		#address-cells = < 0x02 >;
		#size-cells = < 0x02 >;
		reg = < 0x00 0xc1109880 0x00 0x10 >;
		ranges;

		gpio: banks@c11080b0 {
			reg = < 0x0 0xc88344b0 0x0 0x28 >,
				 < 0x0 0xc88344e8 0x0 0x14 >,
				 < 0x0 0xc8834520 0x0 0x14 >,
				 < 0x0 0xc8834430 0x0 0x40 >;
			reg-names = "mux", "pull", "pull-enable", "gpio";
			gpio-controller;
			#gpio-cells = < 0x02 >;
		};

		gpio_ao: ao-bank@c1108030 {
			reg = < 0x0 0xc8100014 0x0 0x8 >,
				 < 0x0 0xc810002c 0x0 0x4 >,
				 < 0x0 0xc8100024 0x0 0x8 >;
			reg-names = "mux", "pull", "gpio";
			gpio-controller;
			#gpio-cells = < 0x02 >;
		};

		external_eth_pins {
			amlogic,setmask = < 0x04 0xfffc00 >;
			amlogic,clrmask = < 0x04 0x3fc 0x03 0xffcff800 >;
			amlogic,pins = "GPIOZ_0", "GPIOZ_1", "GPIOZ_2", "GPIOZ_3", "GPIOZ_4", "GPIOZ_5", "GPIOZ_6",
			"GPIOZ_7", "GPIOZ_8", "GPIOZ_9", "GPIOZ_10", "GPIOZ_11", "GPIOZ_12", "GPIOZ_13";
		};

		internal_eth_pins: internal_eth_pins {
			amlogic,setmask = < 0x04 0x3000000 >;
			amlogic,clrmask = < 0x03 0x300000 >;
			amlogic,pins = "GPIOZ_14", "GPIOZ_15";
		};

		jtag_apao_pins: jtag_apao_pin {
			amlogic,clrmask = < 0x06 0x7fe0000 >;
			amlogic,pins = "GPIOH_6", "GPIOH_7", "GPIOH_8", "GPIOH_9";
		};

		jtag_apee_pins: jtag_apee_pin{
			amlogic,clrmask = < 0x06 0x3c >;
			amlogic,pins = "CARD_0", "CARD_1", "CARD_2", "CARD_3";
		};

		remote_pin {
			amlogic,setmask = < 0x10 0x01 >;
			amlogic,pins = "GPIOAO_7";
		};

		ao_uart {
			amlogic,setmask = < 0x10 0x1800 >;
			amlogic,pins = "GPIOAO_0", "GPIOAO_1";
		};

		ao_b_uart_pins: ao_b_uart {
			amlogic,setmask = < 0x10 0x1800000 >;
			amlogic,pins = "GPIOAO_4", "GPIOAO_5";
		};

		uart_a_pins: a_uart {
			amlogic,setmask = < 0x05 0xf0000 >;
			amlogic,clrmask = < 0x05 0x3c0 >;
			amlogic,pins = "GPIOX_12", "GPIOX_13", "GPIOX_14", "GPIOX_15";
		};

		b_uart_pins: b_uart {
			amlogic,setmask = < 0x02 0x18000 >;
			amlogic,pins = "GPIODV_24", "GPIODV_25";
		};

		c_uart_pins: c_uart {
			amlogic,setmask = < 0x05 0x3000 >;
			amlogic,pins = "GPIOX_8", "GPIOX_9";
		};

		wifi_32k_pins: wifi_32k_pins {
			amlogic,setmask = < 0x05 0x8000 >;
			amlogic,pins = "GPIOX_16";
		};

		sd_clk_cmd_pins: sd_clk_cmd_pins{
			amlogic,setmask = < 0x06 0x0c 0x10 0x1800 >;
			amlogic,pins = "CARD_2", "CARD_3";	/* CARD_2: CLK, CARD_3: CMD */
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_all_pins: sd_all_pins{
			amlogic,setmask = < 0x06 0x3f 0x10 0x1800 >;
			amlogic,clrmask = < 0x06 0xfc0 >;
			amlogic,pins = "CARD_0", "CARD_1", "CARD_2", "CARD_3", "CARD_4", "CARD_5";
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_1bit_pins: sd_1bit_pins{
			amlogic,setmask = < 0x06 0x1c 0x10 0x1800 >;
			amlogic,clrmask = < 0x06 0xcc3 >;
			amlogic,pins = "CARD_1", "CARD_2", "CARD_3";
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_clk_cmd_uart_pins: sd_clk_cmd_uart_pins{
			amlogic,setmask = < 0x06 0x30c >;
			amlogic,clrmask = < 0x06 0xcc3 0x10 0x1800 >;
			amlogic,pins = "CARD_2", "CARD_3";	/* CARD_2: CLK, CARD_3: CMD */
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_1bit_uart_pins: sd_1bit_uart_pins{
			amlogic,setmask = < 0x06 0x31c >;
			amlogic,clrmask = < 0x06 0xcc3 0x10 0x1800 >;
			amlogic,pins = "CARD_1", "CARD_2", "CARD_3";
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_to_ao_uart_pins: sd_to_ao_uart_pins {
			amlogic,setmask = < 0x10 0x1800 >;
			amlogic,clrmask = < 0x06 0x300 >;
			amlogic,pins = "GPIOAO_0", "GPIOAO_1";
			amlogic,enable-output = < 0x01 >;
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		ao_to_sd_uart_pins: ao_to_sd_uart_pins {
			amlogic,setmask = < 0x06 0x300 >;
			amlogic,clrmask = < 0x10 0x1800 0x06 0xcc3 >;
			amlogic,pins = "CARD_4", "CARD_5";
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};
		/* switch jtag pins to card from apao */
		ao_to_sd_jtag_pins: ao_to_sd_jtag_pins{
			amlogic,setmask = < 0x06 0x300 >;	/*set for uart-ee, fixme? */
			amlogic,clrmask = < 0x06 0xcff 0x10 0x1800 >;	/* clear for jtag */ /* clear for uart-ee, fixme? */
			amlogic,pins = "CARD_4", "CARD_5";	/* should be CARD_0/1/2/3 */
			amlogic,enable-output = < 0x01 >;	/* 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sd_to_ao_jtag_pins: sd_to_ao_jtag_pins {
			amlogic,setmask = < 0x10 0x1800 >;
			amlogic,clrmask = < 0x06 0xfc0 >;
			amlogic,pins = "GPIOAO_0", "GPIOAO_1";
			amlogic,enable-output = < 0x01 >;
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		emmc_clk_cmd_pins: emmc_clk_cmd_pins{
			amlogic,setmask = < 0x07 0x60000000 >;
			amlogic,clrmask = < 0x07 0xe0 >;
			amlogic,pins = "BOOT_8", "BOOT_10";	/** BOOT_10: CMD, BOOT_8: CLK */
			amlogic,enable-output = < 0x01 >;	/** 0: output, 1: input */
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		emmc_conf_pull_up: emmc_conf_pull_up{
			amlogic,pins = "BOOT_0", "BOOT_1", "BOOT_2", "BOOT_3", "BOOT_4",
					"BOOT_5", "BOOT_6", "BOOT_7", "BOOT_8", "BOOT_10";
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		emmc_conf_pull_done: emmc_conf_pull_done{
			amlogic,pins = "BOOT_15";
			amlogic,pullup = < 0x00 >;
			amlogic,pullupen = < 0x01 >;
		};

		emmc_all_pins: emmc_all_pins {
			amlogic,setmask = < 0x07 0xf0000000 >;
			amlogic,clrmask = < 0x07 0xe0 >;
			amlogic,pins = "BOOT_0", "BOOT_1", "BOOT_2", "BOOT_3", "BOOT_4", "BOOT_5", "BOOT_6", "BOOT_7", "BOOT_8", "BOOT_10", "BOOT_15";
			amlogic,enable-output = < 0x01 >;
		};

		sdio_clk_cmd_pins: sdio_clk_cmd_pins {
			amlogic,setmask = < 0x05 0xc000000 >;
			amlogic,pins = "GPIOX_4", "GPIOX_5";
			amlogic,enable-output = < 0x01 >;
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		sdio_all_pins: sdio_all_pins {
			amlogic,setmask = < 0x05 0xfc000000 >;
			amlogic,pins = "GPIOX_0", "GPIOX_1", "GPIOX_2", "GPIOX_3", "GPIOX_4", "GPIOX_5";
			amlogic,enable-output = < 0x01 >;
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		conf_nand_pulldown: conf_nand_pulldown {
			amlogic,pins = "BOOT_0", "BOOT_1", "BOOT_2", "BOOT_3", "BOOT_4", "BOOT_5", "BOOT_6", "BOOT_7", "BOOT_15";
			amlogic,pullup = < 0x00 >;
			amlogic,pullupen = < 0x01 >;
		};

		conf_nand_pullup: conf_nand_pullup {
			amlogic,pins = "BOOT_8", "BOOT_10";
			amlogic,pullup = < 0x01 >;
			amlogic,pullupen = < 0x01 >;
		};

		all_nand_pins: all_nand_pins {
			amlogic,setmask = < 0x07 0x800000ff >;
			amlogic,clrmask = < 0x07 0x70ffbc00 >;
			amlogic,pins = "BOOT_0", "BOOT_1", "BOOT_2", "BOOT_3", "BOOT_4", "BOOT_5", "BOOT_6", "BOOT_7", "BOOT_8", "BOOT_9", "BOOT_10", "BOOT_11", "BOOT_12", "BOOT_13", "BOOT_14", "BOOT_15";
			amlogic,enable-output = < 0x01 >;
		};

		nand_cs: nand_cs {
			amlogic,setmask = < 0x07 0xc0 >;
			amlogic,clrmask = < 0x07 0x40040000 >;
			amlogic,pins = "BOOT_8", "BOOT_9";
		};

		hdmitx_hpd: hdmitx_hpd {
			amlogic,setmask = < 0x06 0x80000000 >;
			amlogic,pins = "GPIOH_0";
		};

		hdmitx_ddc: hdmitx_ddc {
			amlogic,setmask = < 0x06 0x30000000 >;
			amlogic,pins = "GPIOH_1", "GPIOH_2";
		};

		hdmitx_aocec: hdmitx_aocec {
