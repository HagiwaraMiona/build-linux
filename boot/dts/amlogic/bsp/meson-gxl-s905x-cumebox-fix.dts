				&clock GCLK_IDX_USB1 >;
		/* GCLK_IDX_USB_GENERAL: 0x3a, GCLK_IDX_MISC_USB1_TO_DDR: 0x48, GCLK_IDX_USB1: 0x36 */
		reset-names = "usb_general", "usb1", "usb1_to_ddr";
	};

	/* AUDIO MESON8 DEVICES */
	i2s_dai: I2S {
		#sound-dai-cells = < 0x00 >;
		resets = < 
			&clock GCLK_IDX_AIU_AI_TOP_GLUE
			&clock GCLK_IDX_AUD_BUF_ABD
			&clock GCLK_IDX_AIU_I2S_OUT
			&clock GCLK_IDX_AIU_AMCLK_MEASURE
			&clock GCLK_IDX_AIU_AIFIFO2
			&clock GCLK_IDX_AIU_AUD_MIXER
			&clock GCLK_IDX_AIU_MIXER_REG
			&clock GCLK_IDX_AIU_ADC
			&clock GCLK_IDX_AIU_TOP_LEVEL
			&clock GCLK_IDX_AIU_AOCLK
			&clock GCLK_IDX_AUD_IN
		 >;
		reset-names = "top_glue", 
			"aud_buf",
			"i2s_out",
			"amclk_measure",
			"aififo2",
			"aud_mixer",
			"mixer_reg",
			"adc",
			"top_level",
			"aoclk",
			"aud_in";
		clocks = < &clock CLK_MPLL2 >,
			< &clock CLK_AMCLK >;
		clock-names = "mpll2", "mclk";
		compatible = "amlogic, aml-i2s-dai";
	};

	spdif_dai: SPDIF {
		#sound-dai-cells = < 0 >;
		compatible = "amlogic, aml-spdif-dai";
		resets = < &clock GCLK_IDX_AIU_IEC958
			&clock GCLK_IDX_AIU_ICE958_AMCLK >;
		/* GCLK_IDX_AIU_IEC958: 0x27, GCLK_IDX_AIU_ICE958_AMCLK: 0x90 */
		reset-names = "iec958", "iec958_amclk";
		clocks = < &clock CLK_MPLL1 >,
			< &clock CLK_I958 >,
			< &clock CLK_AMCLK >,
			< &clock CLK_SPDIF >,
			< &clock CLK_81 >;
		clock-names = "mpll1", "i958", "mclk", "spdif", "clk_81";
	};

	pcm_dai: PCM {
		#sound-dai-cells = < 0x00 >;
		compatible = "amlogic, aml-pcm-dai";
		pinctrl-names = "aml_audio_btpcm";
		pinctrl-0 = < &audio_btpcm_pins >;
		clocks = < &clock CLK_MPLL0 >,
			< &clock CLK_PCM_MCLK >,
			< &clock CLK_PCM_SCLK >;
		clock-names = "mpll0", "pcm_mclk", "pcm_sclk";
		pcm_mode = < 0x01 >;	/* 0 = slave mode, 1 = master mode */
	};

	i2s_plat: i2s_platform {
		compatible = "amlogic, aml-i2s";
		interrupts = < 0x00 0x1d 0x01 >;
	};

	pcm_plat: pcm_platform {
		compatible = "amlogic, aml-pcm";
	};

	spdif_codec: spdif_codec{
		#sound-dai-cells = < 0x00 >;
		compatible = "amlogic, aml-spdif-codec";
		pinctrl-names = "aml_audio_spdif";
		pinctrl-0 = < &audio_spdif_pins >;
	};

	pcm_codec: pcm_codec{
		#sound-dai-cells = < 0x00 >;
		compatible = "amlogic, pcm2BT-codec";
	};
	/* endof AUDIO MESON8 DEVICES */

	/* AUDIO board specific */
	dummy_codec: dummy{
		#sound-dai-cells = < 0x00 >;
		compatible = "amlogic, aml_dummy_codec";
		status = "disable";
	};

	amlogic_codec: t9015{
		#sound-dai-cells = < 0x00 >;
		compatible = "amlogic, aml_codec_T9015";
		reg = < 0x00 0xc8832000 0x00 0x14 >;
		status = "okay";
	};

	aml_m8_snd {
		compatible = "aml, aml_snd_m8";
		status = "okay";
		aml-sound-card,format = "i2s";
		aml_sound_card,name = "AML-M8AUDIO";
		aml,audio-routing = "Ext Spk", "LOUTL", "Ext Spk", "LOUTR";

		mute_gpio-gpios = < &gpio GPIOH_5 0 >; /* GPIOH_5: 0x15 */
		mute_inv;
		sleep_time = < 0x64 >;
		hp_disable;
		hp_paraments = < 0x320 0x12c 0x00 0x05 0x01 >;
		pinctrl-names = "aml_snd_m8";
		pinctrl-0 = < &audio_pins >;
		cpu_list = < &cpudai0 &cpudai1 &cpudai2 >;
		codec_list = < &codec0 &codec1 &codec2 >;
		plat_list = < &i2s_plat &i2s_plat &pcm_plat >;

		cpudai0: cpudai0 {
			sound-dai = < &i2s_dai >;
		};

		cpudai1: cpudai1 {
			sound-dai = < &spdif_dai >;
		};

		cpudai2: cpudai2 {
			sound-dai = < &pcm_dai >;
		};

		codec0: codec0 {
			sound-dai = < &amlogic_codec >;
		};

		codec1: codec1 {
			sound-dai = < &spdif_codec >;
		};

		codec2: codec2 {
			sound-dai = < &pcm_codec >;
		};
	};
	/* END OF AUDIO board specific */

	gpio_keypad {
		compatible = "amlogic, gpio_keypad";
		status = "okay";
		scan_period = < 0x14 >;
		key_num = < 0x03 >;
		key_name = "resetkey", "wifiap", "power";
		key_code = < 0x57 0x58 0x44 >;
		key_pin = < &gpio_ao GPIOAO_2 GPIO_ACTIVE_HIGH >;	/*GPIOAO_2: 0x02*/
		irq_keyup = < 0x06 >;
		irq_keydown = < 0x07 >;
	};

	aml_sensor0: aml-sensor@0 {
		compatible = "amlogic, aml-thermal";
		device_name = "thermal";
		#thermal-sensor-cells = < 0x01 >;
		cooling_devices {

			cpufreq_cool_cluster0 {
				min_state = < 0xf4240 >; //1000000
				dyn_coeff = < 0x8c >; //140
				cluster_id = < 0x00 >;
				node_name = "cpus";
				device_type = "cpufreq";
			};

			cpucore_cool_cluster0 {
				min_state = < 0x01 >;
				dyn_coeff = < 0x00 >;
				cluster_id = < 0x00 >;
				node_name = "cpu_core_cluster0";
				device_type = "cpucore";
			};

			gpufreq_cool {
				min_state = < 0x190 >; //400
				dyn_coeff = < 0x1b5 >; //437
				cluster_id = < 0x00 >;
				node_name = "mali";
				device_type = "gpufreq";
			};

			gpucore_cool {
				min_state = < 0x01 >;
				dyn_coeff = < 0x00 >;
				cluster_id = < 0x00 >;
				node_name = "thermal_gpu_cores";
				device_type = "gpucore";
			};
		};

		cpu_cluster0: cpu_core_cluster0 {
			#cooling-cells = < 0x02 >;	/* min followed by max */
		};

		gpucore: thermal_gpu_cores {
			#cooling-cells = < 0x02 >;	/* min followed by max */
		};
	};

	thermal-zones {

		soc_thermal {
			polling-delay = < 0x3e8 >; //1000
			polling-delay-passive = < 0x64 >; //100
			sustainable-power = < 0x866 >; //2150
			thermal-sensors = < &aml_sensor0 3 >;

			trips {

				switch_on: trip-point@0 {
					temperature = < 0x11170 >; //70000
					hysteresis = < 0x3e8 >; //1000
					type = "passive";
				};

				control: trip-point@1 {
					temperature = < 0x13880 >; //80000
					hysteresis = < 0x3e8 >; //1000
					type = "passive";
				};

				hot: trip-point@2 {
					temperature = < 0x14c08 >; //85000
					hysteresis = < 0x1388 >; //5000
					type = "hot";
				};

				critical: trip-point@3 {
					temperature = < 0x3f7a0 >; //260000
					hysteresis = < 0x3e8 >; //1000
					type = "critical";
				};
			};

			cooling-maps {

				cpufreq_cooling_map {
					trip = < &control >;
					cooling-device = < &cpus 0 4 >;
					contribution = < 0x400 >; //1024
				};

				cpucore_cooling_map {
					trip = < &control >;
					cooling-device = < &cpu_cluster0 0 3 >;
					contribution = < 0x400 >; //1024
				};

				gpufreq_cooling_map {
					trip = < &control >;
					cooling-device = < &gpu 0 4 >;
					contribution = < 0x400 >; //1024
				};

				gpucore_cooling_map {
					trip = < &control >;
					cooling-device = < &gpucore 0 2 >;
					contribution = < 0x400 >; //1024
				};
			};
		};
	};

	ledlight {
		compatible = "amlogic, ledlight";
		dev_name = "ledlight";
		status = "okay";
		power_gpio_0 = < &power_gpio_b >;
		power_gpio_1 = < &power_gpio_r >;
		power_gpio_2 = < &power_gpio_g >;
		power_gpio_3 = < &power_gpio_breath >;

		power_gpio_b: power_gpio_b {
			led_name = "blue_led";
			led_gpio = < &gpio GPIOW_10 GPIO_ACTIVE_HIGH >; /* GPIOW_10: 0x49 */
			led_active = < GPIO_ACTIVE_LOW >;
			led_type = [ 42 00 ];
		};

		power_gpio_r: power_gpio_r {
			led_name = "red_led";
			led_gpio = < &gpio GPIOW_12 GPIO_ACTIVE_HIGH >; /* GPIOW_12: 0x4b */
			led_active = < GPIO_ACTIVE_LOW >;
			led_type = [ 52 00 ];
		};

		power_gpio_g: power_gpio_g {
			led_name = "green_led";
			led_gpio = < &gpio GPIOW_13 GPIO_ACTIVE_HIGH >; /* GPIOW_13: 0x4c */
			led_active = < GPIO_ACTIVE_LOW >;
			led_type = [ 47 00 ];
		};

		power_gpio_breath: power_gpio_breath {
			led_name = "breath";
			led_gpio = < &gpio_ao GPIOAO_3 GPIO_ACTIVE_HIGH >; /* GPIOAO_3: 0x03 */
			led_active = < GPIO_ACTIVE_LOW >;
			led_type = [ 54 00 ];
		};
	};
};
