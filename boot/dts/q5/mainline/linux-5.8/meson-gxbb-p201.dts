/* eMMC */
&sd_emmc_c {
	status = "okay";
	pinctrl-0 = <&emmc_pins>, <&emmc_ds_pins>;
	pinctrl-1 = <&emmc_clk_gate_pins>;
	reg = <0x0 0x74000 0x0 0x2000>;
	pinctrl-names = "default", "clk-gate";

	bus-width = <8>;
	max-frequency = <200000000>;
	non-removable;
	disable-wp;
	cap-mmc-highspeed;
	mmc-ddr-1_8v;
	mmc-hs200-1_8v;

	mmc-pwrseq = <&emmc_pwrseq>;
	// vmmc-supply = <&vcc3v3>;
	// vqmmc-supply = <&vcc1v8>;
};

&usb0_phy {
	status = "okay";
	phy-supply = <&vdd_5v>;
};

&usb1_phy {
	status = "okay";
};

&usb0 {
	status = "okay";
	clock-names = "usb1";
};

&usb1 {
	status = "okay";
	clock-names = "usb2";
};

&aiu {
	status = "okay";
};

&saradc {
	status = "okay";
	vref-supply = <&vddio_ao18>;
};

&ir {
	status = "okay";
};
